<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextClock">
        <attr name="format12Hour" format="string" />
        <attr name="format24Hour" format="string" />
        <attr name="formatTV" format="string" />
        <attr name="timeZone" format="string" />
    </declare-styleable>

    <declare-styleable name="RowApp">
        <attr name="rowisAnimateApp" format="boolean" />
        <attr name="rowbackgroundResource" format="reference" />
        <attr name="rowimageviewBackgroundResource" format="reference" />
        <attr name="rowtitle" format="reference" />
        <attr name="rowcomponentPKG" format="string" />
        <attr name="rowcomponentCLS" format="string" />

    </declare-styleable>


    <declare-styleable name="NetworkImageView">
        <attr name="exampleString" format="string" />
        <attr name="exampleDimension" format="dimension" />
        <attr name="exampleColor" format="color" />
        <attr name="exampleDrawable" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="BluetoothImageView">
        <attr name="bluetoothDrawable" format="color|reference" />
    </declare-styleable>
</resources>